def sfd(n,facs):
    s = 0
    while n > 0:
      s += facs[n % 10]
      n //= 10
    return s

def chain(n,facs):
    nums = [n]
    next = sfd(n,facs)
    while next not in nums:
        nums.append(next)
        next = sfd(next,facs)
    return len(nums)
    
facs = [1]
for k in range(1,10):
    facs.append(facs[-1]*k)
len60 = 0

for k in range(1,10**6):
    if chain(k,facs) == 60:
        len60 += 1

print(len60)

